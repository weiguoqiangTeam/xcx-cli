{
	"name": "xcx-cli",
	"version": "1.0.0",
	"description": "测试Ci自动上传",
	"main": ".eslintrc.js",
	"scripts": {
		"start": "rm -rf ./dist && npm install && cross-env NODE_ENV=development gulp",
		"start-pro": "rm -rf ./dist && cross-env NODE_ENV=production gulp",
		"preview": "npm run build-dev && gulp preview",
		"preview-pro": "npm run build-pro && gulp preview",
		"build-pro": "rm -rf ./dist && cross-env NODE_ENV=production gulp build",
		"build-dev": "rm -rf ./dist && cross-env NODE_ENV=development gulp build",
		"build-npm": "gulp npm",
		"deploy-dev": "git push && npm run build-dev && NODE_ENV=development gulp upload",
		"deploy-pro": "git push && npm run build-pro && NODE_ENV=production gulp upload",
		"clearBranch": "git remote update origin --prune && gulp clearBranch",
		"cp": "create-wxapp-page --type=page --dir=src/ --indent=2 --style=less",
		"cc": "create-wxapp-page --type=component --dir=src/ --indent=2 --style=less"
	},
	"dependencies": {
		"@vant/weapp": "^1.10.2",
		"dayjs": "^1.11.0"
	},
	"devDependencies": {
		"@babel/core": "^7.12.9",
		"@babel/preset-env": "^7.12.7",
		"@babel/register": "^7.12.1",
		"@commitlint/cli": "^14.1.0",
		"@commitlint/config-conventional": "^14.1.0",
		"autoprefixer": "^6.7.7",
		"babel-plugin-lodash": "^3.3.4",
		"create-wxapp-page": "^2.0.9",
		"cross-env": "^7.0.3",
		"del": "^6.0.0",
		"eslint": "^8.2.0",
		"file-exists": "^5.0.1",
		"gulp": "^4.0.2",
		"gulp-autoprefixer": "^8.0.0",
		"gulp-babel": "^8.0.0",
		"gulp-cached": "^1.1.1",
		"gulp-clean-css": "^4.3.0",
		"gulp-cssnano": "^2.1.3",
		"gulp-debug": "^4.0.0",
		"gulp-if": "^3.0.0",
		"gulp-imagemin": "^7.1.0",
		"gulp-less": "^4.0.1",
		"gulp-newer": "^1.4.0",
		"gulp-path-alias": "^1.2.1",
		"gulp-postcss": "^9.0.0",
		"gulp-px2rpx": "^0.2.2",
		"gulp-qcloud-upload": "^2.2.0",
		"gulp-rename": "^2.0.0",
		"gulp-replace": "^1.0.0",
		"gulp-sourcemaps": "^3.0.0",
		"gulp-uglify": "^3.0.2",
		"gulp-util": "^3.0.8",
		"lint-staged": "^12.0.2",
		"miniprogram-ci": "^1.0.94",
		"postcss-px2rpx": "^0.0.4",
		"postcss-px2rpx-transform": "^1.0.2",
		"prettier": "^2.4.1",
		"qcloud-upload": "^1.5.0",
		"yargs": "^7.1.1"
	},
	"repository": {
		"type": "git",
		"url": "https://gitee.com/wei_guo_qiang/xcx-cli.git"
	},
	"author": "waim",
	"license": "ISC"
}
